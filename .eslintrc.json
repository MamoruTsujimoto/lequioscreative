{
	"env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["next/core-web-vitals", "prettier"],
	"settings": {
    "react": {
      "version": "detect"
    }
  },
	"parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "project": "./tsconfig.json",
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
	"plugins": ["simple-import-sort", "import"]
}
